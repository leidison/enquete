<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * AvailableRange
 *
 * @ORM\Table(name="available_range", indexes={@ORM\Index(name="fk_available_range_clinic1_idx", columns={"clinic_id"})})
 * @ORM\Entity
 */
class AvailableRange
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day", type="boolean", nullable=true)
     */
    private $weekDay;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="initial_hour1", type="time", nullable=false)
     */
    private $initialHour1;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="final_hour1", type="time", nullable=false)
     */
    private $finalHour1;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="initial_hour2", type="time", nullable=true)
     */
    private $initialHour2;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="final_hour2", type="time", nullable=true)
     */
    private $finalHour2;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="initial_hour3", type="time", nullable=true)
     */
    private $initialHour3;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="final_hour3", type="time", nullable=true)
     */
    private $finalHour3;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day1", type="boolean", nullable=true)
     */
    private $weekDay1;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day2", type="boolean", nullable=true)
     */
    private $weekDay2;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day3", type="boolean", nullable=true)
     */
    private $weekDay3;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day4", type="boolean", nullable=true)
     */
    private $weekDay4;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day5", type="boolean", nullable=true)
     */
    private $weekDay5;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day6", type="boolean", nullable=true)
     */
    private $weekDay6;

    /**
     * @var boolean
     *
     * @ORM\Column(name="week_day7", type="boolean", nullable=true)
     */
    private $weekDay7;

    /**
     * @var \Clinic
     *
     * @ORM\ManyToOne(targetEntity="Clinic")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="clinic_id", referencedColumnName="id")
     * })
     */
    private $clinic;


}
