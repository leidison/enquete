<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * HealthInsurance
 *
 * @ORM\Table(name="health_insurance", indexes={@ORM\Index(name="fk_health_insurance_health_insurance_agency1_idx", columns={"health_insurance_agency_id"})})
 * @ORM\Entity
 */
class HealthInsurance
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="name", type="string", length=100, nullable=false)
     */
    private $name;

    /**
     * @var boolean
     *
     * @ORM\Column(name="status", type="boolean", nullable=false)
     */
    private $status;

    /**
     * @var \HealthInsuranceAgency
     *
     * @ORM\ManyToOne(targetEntity="HealthInsuranceAgency")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="health_insurance_agency_id", referencedColumnName="id")
     * })
     */
    private $healthInsuranceAgency;


}
