<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * UnavailableRange
 *
 * @ORM\Table(name="unavailable_range", indexes={@ORM\Index(name="fk_unavailable_range_clinic1_idx", columns={"clinic_id"})})
 * @ORM\Entity
 */
class UnavailableRange
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="start", type="datetime", nullable=false)
     */
    private $start;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="finish", type="datetime", nullable=true)
     */
    private $finish;

    /**
     * @var \Clinic
     *
     * @ORM\ManyToOne(targetEntity="Clinic")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="clinic_id", referencedColumnName="id")
     * })
     */
    private $clinic;


}
