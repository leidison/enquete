<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oauth2Client" table="oauth2_client">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="randomId" type="string" column="random_id" length="255" nullable="false"/>
    <field name="redirectUris" type="text" column="redirect_uris" nullable="false"/>
    <field name="secret" type="string" column="secret" length="255" nullable="false"/>
    <field name="allowedGrantTypes" type="text" column="allowed_grant_types" nullable="false"/>
  </entity>
</doctrine-mapping>
